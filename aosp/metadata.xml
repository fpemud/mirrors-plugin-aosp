<?xml version="1.0" encoding="utf-8"?>
<plugin id="aosp">
  <git-mirror id="aosp">
    <name>AOSP</name>
    <data-directory>aosp</data-directory>
    <initializer>
      <executable>initializer.py</executable>
    </initializer>
    <updater>
      <executable>updater.py</executable>
      <cron-expression>0 0 * * 0</cron-expression>
    </updater>
    <advertiser>
      <protocol>git-http</protocol>
    </advertiser>
  </git-mirror>
</plugin>
