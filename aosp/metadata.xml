<?xml version="1.0" encoding="utf-8"?>
<plugin id="aosp">
  <mirror-site id="aosp">
    <name>AOSP</name>
    <storage type="file"/>
    <advertiser type="git"/>
    <initializer>
      <executable>initializer.py</executable>
    </initializer>
    <updater>
      <executable>updater.py</executable>
      <schedule type="interval">1d</schedule>
    </updater>
  </mirror-site>
</plugin>
